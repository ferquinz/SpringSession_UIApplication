  "license": "MIT",
  "homepage": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "~2.2.4",
    "angular": "~1.4.9",
    "bootstrap": "~3.3.7",
    "angular-route": "~1.4.9"
  },
  "install": {
    "base":  "static",
    "path": {
      "js": "{name}/js",
      "css": "{name}/css",
      "eot": "{name}/fonts",
      "svg": "{name}/fonts",
      "ttf": "{name}/fonts",
      "woff": "{name}/fonts"
    },
    "sources": {
      "bootstrap": [
        "src/main/resources/static/bootstrap/dist/js/*.js",
        "src/main/resources/static/bootstrap/dist/css/*.css",
        "src/main/resources/static/bootstrap/dist/fonts/*"
      ]
    }
  }
}
